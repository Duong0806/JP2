          <TableColumn fx:id="tbId" prefWidth="48.0" text="ID" />
          <TableColumn fx:id="tbName" prefWidth="212.0" text="NAME" />
            <TableColumn fx:id="tbAuthor" prefWidth="217.0" text="AUTHOR" />
            <TableColumn fx:id="tbQty" prefWidth="53.0" text="QTY" />
        </columns>
      </TableView>
      <Button onAction="#goToBack" layoutX="19.0" layoutY="30.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="97.0" text="&lt;&lt; BACK" />
